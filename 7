 #include <stdio.h>
#include<string.h>
#include<stdlib.h>
 
int main()
{
  char a;
  printf("Enter a character...:");
  scanf("%c",&a);
 
  if(a>='A' && a<='Z')
  {
  printf("%c is a Alphabet",a);
  }
 
  if(a>='a' && a<='z')
  {
  printf("%c is a string",a);
  }
 if(a>=0 && a<=99)
 {
  printf("it is digit");
 }
 
 
  return 0;
}
